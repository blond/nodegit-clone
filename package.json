{
  "name": "nodegit-clone",
  "version": "1.0.7",
  "description": "Clone git repository with nodegit",
  "license": "MIT",
  "repository": "blond/nodegit-clone",
  "author": "Andrew Abramov <mail@blond.im> (github.com/blond)",
  "keywords": [
    "git",
    "clone",
    "http",
    "https",
    "ssh",
    "github",
    "token"
  ],
  "main": "lib/clone.js",
  "files": [
    "lib/**",
    "index.js"
  ],
  "engines": {
    "node": ">= 4.0"
  },
  "dependencies": {
    "is-ssh": "1.3.0",
    "nodegit": "0.12.2",
    "throw": "1.1.0"
  },
  "devDependencies": {
    "ava": "0.14.0",
    "babel-cli": "6.7.7",
    "babel-eslint": "6.0.4",
    "babel-preset-nodejs-lts": "2.0.1",
    "babel-register": "6.8.0",
    "coveralls": "2.11.9",
    "es6-promisify": "4.0.0",
    "eslint": "2.9.0",
    "eslint-config-pedant": "0.3.0",
    "fs-extra": "0.30.0",
    "nyc": "6.4.2",
    "tempfile": "1.1.1"
  },
  "scripts": {
    "test": "npm run lint && npm run cover",
    "lint": "eslint .",
    "unit-test": "ava",
    "cover": "nyc ava",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "prepublish": "npm run clean && npm run compile",
    "compile": "babel src -d lib",
    "clean": "rm -rf lib/"
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/fixtures/**",
      "!test/utils/**"
    ],
    "verbose": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
