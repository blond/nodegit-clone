{
  "name": "nodegit-clone",
  "version": "1.2.0",
  "description": "Clone git repository with nodegit",
  "license": "MIT",
  "repository": "blond/nodegit-clone",
  "author": "Andrew Abramov <mail@blond.im> (github.com/blond)",
  "keywords": [
    "git",
    "clone",
    "http",
    "https",
    "ssh",
    "github",
    "token"
  ],
  "main": "lib/clone.js",
  "files": [
    "lib/**",
    "index.js"
  ],
  "engines": {
    "node": ">= 4.0"
  },
  "dependencies": {
    "is-ssh": "1.3.0",
    "nodegit": "0.16.0",
    "throw": "1.1.0"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "coveralls": "^2.11.9",
    "del": "^2.2.1",
    "es6-promisify": "^5.0.0",
    "eslint": "^3.0.0",
    "eslint-config-pedant": "^0.8.0",
    "nyc": "^9.0.1",
    "output-file": "^1.1.1",
    "tempfile": "^1.1.1"
  },
  "scripts": {
    "test": "npm run lint && npm run cover",
    "lint": "eslint .",
    "unit-test": "ava",
    "cover": "nyc ava",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/fixtures/**",
      "!test/utils/**"
    ],
    "verbose": true
  }
}
